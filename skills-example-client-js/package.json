{
  "name": "skills-example-client-js",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "description": "SkillTree Client Lib Integration Tests - Native JS",
  "main": "index.js",
  "scripts": {
    "postinstall": "node copyDeps.js",
    "serve": "npx live-server --port=8092 --open=app --cors --proxy=/api:http://localhost:8090/api --proxy=/native:http://localhost:8092/app --proxy=/assets:http://localhost:8092/app/assets",
    "test": "npm run test",
    "licenseDepCheck": "license-checker --production --onlyAllow='MIT;ISC;Apache-2.0' --summary",
    "licenseHeaderCheck": "license-check-and-add check -f '../license-add/license-add-config.json'",
    "licenseHeaderAdd": "license-check-and-add add -f ../license-add/license-add-config.json",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck",
    "build": "npm run licenseCheck && echo nothingelsetodo"
  },
  "author": "SkillTree Team",
  "dependencies": {
    "@skills/skills-client-configuration": "1.1.0",
    "@skills/skills-client-js": "1.1.1",
    "@skills/skills-client-reporter": "1.1.1",
    "axios": "^0.19.0",
    "bootstrap": "4.3.1",
    "jquery": "^3.4.1",
    "js-beautify": "1.10.2"
  },
  "devDependencies": {
    "fs-extra": "^8.1.0",
    "license-check-and-add": "^3.0.4",
    "license-checker": "^25.0.1",
    "live-server": "1.2.1"
  }
}
